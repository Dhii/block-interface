{
    "name": "dhii/output-block-interface",
    "description": "Interfaces for output blocks.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dhii Team",
            "email": "development@dhii.co"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^5.3 | ^7.0",
        "dhii/stringable-interface": "^0.1",
        "dhii/output-renderer-interface": "dev-task/bring-up-to-dhii-standards"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "ptrofimov/xpmock": "^1.1",
        "dhii/php-cs-fixer-config": "dev-php-5.3",
        "codeclimate/php-test-reporter": "<=0.3.2"
    },
    "autoload": {
        "psr-4": {
            "Dhii\\Output\\": "src"
        }
    },
    "scripts": {
        "test": "phpunit",
        "csfix": "php-cs-fixer fix -vvv"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Dhii/output-renderer-interface"
        }
    ]
}
